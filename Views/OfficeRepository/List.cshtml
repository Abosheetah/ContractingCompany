@model IQueryable<OfficeRepository>
<h3 class="p-2 bg-primary text-white text-center">قائمة  جرد المخزن العام</h3> 
<form  asp-action="UpdateAll" method="POST"> 
    <div class="container-fluid">
        <table class="table table-sm table-striped">
            <thead>
                <tr>
                <th scope="col" hidden>ID</th>
                <th scope="col">Product</th>
                <th scope="col">Code</th>
                <th scope="col">Barcode</th>
                <th scope="col">Quantity</th>
                </tr>
            </thead>
        @foreach (var p in ViewBag.Products)
        {         
            <tr>
                <td asp-for="" scope="row" hidden>@p.ID</td>                
                <td name="@p.Name" value="@p.Name">@p.Name</td>
                <td name="@p.Code" value="@p.Code">@p.Code</td>
                <td name="@p.Barcode" value="@p.Barcode">@p.Barcode</td>
                <td><input  class="form-control" value="0"></td>            
            </tr>
        }
        </table>
        <div class="text-center m-2">
            <button type="submit" class="btn btn-primary">Save All</button>
            <a asp-action="List" class="btn btn-secondary">Cancel</a>
        </div>
    </div>  
</form>

